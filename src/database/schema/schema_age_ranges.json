{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Age Ranges",
  "description": "Age range specifications",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "age_range": {
        "type": "string",
        "description": "StartAge - EndAge"
      },
      "age_description": {
        "type": "string",
        "description": "Description of age range"
      }
    },
    "required": [ "age_range", "age_description" ]
  }
}
