{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Auth User Roles",
  "description": "Auth user roles",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string",
        "description": "User ID; typically 4 chars"
      },
      "role": {
        "type": "string",
        "description": "Role assigned to user"
      }
    },
    "required": ["userId", "role"]
  }
}
